package Assignment_DayThree.secquestion;

public class Emp_Excep {

	public static void main(String[] args) throws MyOwnException{
		int yearlySalary = 1000;
		if(yearlySalary<10000) {
			throw new MyOwnException("Salary is less than 10000");
		}
		else 
		{
				System.out.println("Salary is not less than 10000");
			}
	}
}